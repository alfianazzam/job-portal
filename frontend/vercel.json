{
  "routes": [
    {
      "src": "/favicon.ico",
      "headers": {
        "Cache-Control": "public, max-age=86400, immutable"
      }
    },
    {
      "src": "/static/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ]
}
